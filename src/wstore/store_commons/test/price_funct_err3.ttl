@prefix sp: <http://spinrdf.org/sp#> . 


<http://localhost/pricefunct>
    a spin:Function ;
    rdfs:label "An example price function" ;
    spin:body [
        a sp:Construct ;
        sp:resultVariables ([ sp:varName "price"]) ;
        sp:where ([
            sp:subject <http://localhost/variable_constant> ;
            sp:predicate price:hasValue ;
            sp:object [
                sp:varName "caux1"
            ]
        ] [
            sp:subject [
                sp:varName "caux1"
            ] ;
            sp:predicate gr:hasValueFloat ;
            sp:object [
                sp:varName "constant"
            ]
        ] [
            sp:subject [
                sp:varName "caux2"
            ] ;
            sp:predicate gr:hasValueFloat ;
            sp:object [
                sp:varName "usage"
            ]
        ] [
            sp:subject <http://localhost/variable_usage> ;
            sp:predicate price:hasValue ;
            sp:object [
                sp:varName "caux2"
            ]
        ] [
            a sp:Bind ;
            sp:expression [
                a sp:Sum ;
                sp:arg1 [
                    sp:varName "usage"
                ] ;
                sp:arg2 [
                    a sp:Mul ;
                    sp:arg1 [
                        a sp:Mul ;
                        sp:arg1 [
                            sp:varName "constant"
                        ] ;
                        sp:arg2 [
                            sp:varName "constant"
                        ] ;
                    ];
                    sp:arg2 [
                        sp:varName "usage"
                    ]
                ]
            ] ;
            sp:variable [
                sp:varName "price"
            ]
        ]) 
    ] ;
    price:hasVariable
        <http://localhost/variable_usage>, <http://localhost/variable_constant> .

<http://localhost/variable_usage>
    a price:Usage ;
    rdfs:label "Usage variable" .

<http://localhost/variable_constant>
    a price:Constant ;
    rdfs:label "Constant" ;
    price:hasValue [
        a gr:QuantitativeValue ;
        gr:hasValueFloat "5.0"
    ] .
